generic
   type Tipo is (<>);  --  El tipo es discreto
package Generic_Io is
   
   function Try_Convert(Input : in String;
                     Output : out Tipo) return Boolean;
   
   function Get_Discrete(Prompt : String := "Ingresa un número.";
                         Error : String := "No ha ingresado un numero, vuelva a intentarlo") return Tipo;
   

end Generic_Io;
